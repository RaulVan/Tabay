{
  "manifest_version": 3,
  "name": "Tabay",
  "version": "1.0.1",
  "description": "一个强大的标签页管理工具，帮助你更好地组织浏览器标签",
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "notifications",
    "windows"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "fullpage.html",
        "styles.css",
        "popup.js",
        "fullpage.js",
        "icons/*",
        "default-favicon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}